<p id="notice"><%= notice %></p>

<h1>Listing Selling Values</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @selling_values.each do |selling_value| %>
      <tr>
        <td><%= selling_value.value %></td>
        <td><%= link_to 'Show', product_selling_value_path(@product.id, selling_value) %></td>
        <td><%= link_to 'Edit', edit_product_selling_value_path(@product.id, selling_value) %></td>
        <td><%= link_to 'Destroy', product_selling_value_path(@product.id, selling_value), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Selling value', new_product_selling_value_path %>
